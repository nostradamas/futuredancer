<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fdw.mybatis.mapper.StepMapper">

	<select id="selectSteps" resultType="StepBean">
		SELECT * 
		FROM step 
		where hid = #{hid};
	</select>

	
	
</mapper>